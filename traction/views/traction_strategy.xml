<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="traction_strategy_form_view" model="ir.ui.view">
        <field name="name">traction.strategy.form</field>
        <field name="model">traction.strategy</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name"/>
                        </h1>
                        <group>
                            <field name="target_market"/>
                            <field name="uniques"/>
                            <field name="proven_process"/>
                            <field name="guarantee"/>
                        </group>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <record id="traction_strategy_tree_view" model="ir.ui.view">
        <field name="name">traction.strategy.tree</field>
        <field name="model">traction.strategy</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>