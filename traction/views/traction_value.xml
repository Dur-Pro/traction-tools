<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="traction_code_value_view_form" model="ir.ui.view">
        <field name="name">traction.core.value.view.form</field>
        <field name="model">traction.value</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="traction_core_value_view_tree" model="ir.ui.view">
        <field name="name">traction.core.value.view.tree</field>
        <field name="model">traction.value</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>